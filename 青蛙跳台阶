青蛙跳台阶
    Scanner sc = new Scanner(System.in);
    System.out.println("请输入数字");
    int num = sc.nextInt();
    int ret = fun(num);
    System.out.println(ret);
    }
    public static int fun (int num) {
        if (num ==1) {
            return 1;
        } else if (num == 2) {
            return 2;
        } else {
            return fun (num - 1) + fun (num - 2);
        }
