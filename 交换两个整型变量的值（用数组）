public class Testc {
    public static void main(String[] args) {
        //交换两个整型变量的值（用数组）
        int [] arr = {10, 20};
        swap (arr);
        System.out.println("a = " + arr[0] + " b = " + arr[1]);
    }
    public static void swap (int [] arr) {
        int tmp = arr[0];
        arr[0] = arr[1];
        arr[1] = tmp;
    }
}
